                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:text="Menu"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/rounded_corner"
                        android:orientation="vertical"
                        android:padding="@dimen/activity_vertical_margin" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/TextView18"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:text="Locu username:" />

                            <EditText
                                android:id="@+id/EditText01"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="14"
                                android:text="joseph@31tenlounge.com"
                                android:textAppearance="?android:attr/textAppearanceSmall" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/TextView19"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:text="Locu password:" />

                            <EditText
                                android:id="@+id/EditText02"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="14"
                                android:inputType="textPassword"
                                android:text="31tenlounge"
                                android:textAppearance="?android:attr/textAppearanceSmall" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/textView1"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:text="Locu ID:" />

                            <EditText
                                android:id="@+id/locuId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="14"
                                android:text="8d084f80fd818d6bcf89"
                                android:textAppearance="?android:attr/textAppearanceSmall" />

                        </LinearLayout>

                        <!--  8d084f80fd818d6bcf89 - 31ten -->
                        <!--  5a0999dda39f9fe07a44 - Chaya Venice -->
                        <!--  49be3dcf7e507f6b4775 - Joe's restaurant -->
                        <!--  beec9320f3921035e4d7 - Finn McCool's -->

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/TextView17"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:text="Locu section" />

                            <EditText
                                android:id="@+id/locuSectionEditText"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="14"
                                android:hint="The section name under which the bar selection is listed"
                                android:text="Bar"
                                android:textAppearance="?android:attr/textAppearanceSmall" />
                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/TextView20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:text="Orders"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/rounded_corner"
                        android:orientation="vertical"
                        android:padding="@dimen/activity_vertical_margin" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/textView1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Order timeout:" />

                            <EditText
                                android:id="@+id/orderTimeOut"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:ems="3"
                                android:gravity="right|center_vertical"
                                android:hint="Time in min before an order in each state times out"
                                android:inputType="numberDecimal"
                                android:text="15"
                                android:textAppearance="?android:attr/textAppearanceSmall" />

                            <TextView
                                android:id="@+id/TextView16"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/activity_horizontal_margin"
                                android:text="minutes" />

                            <TextView
                                android:id="@+id/textView1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                                android:text="Total tax rate:" />

                            <EditText
                                android:id="@+id/taxRateEdit"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:ems="3"
                                android:gravity="right|center_vertical"
                                android:inputType="numberDecimal"
                                android:text="9.5"
                                android:textAppearance="?android:attr/textAppearanceSmall" />

                            <TextView
                                android:id="@+id/TextView15"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="%" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/TextView21"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:text="Printer IP address:" />

                            <EditText
                                android:id="@+id/printer_ip_address"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="14"
                                android:hint="192.168.1.4"
                                android:text="192.168.1.4"
                                android:textAppearance="?android:attr/textAppearanceSmall" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/order_options"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal" >

                                <TextView
                                    android:id="@+id/TextView23"
                                    android:layout_width="120dp"
                                    android:layout_height="wrap_content"
                                    android:text="Display options:" />

                                <CheckBox
                                    android:id="@+id/show_orders_in_progress"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
